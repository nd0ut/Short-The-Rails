app.config(function ($routeProvider, $locationProvider, Session) {
    $routeProvider
        .when('/sign_in', {
            templateUrl: '<%= asset_path("sessions/new.html.erb") %>',
            controller: 'SessionsCtrl'
        })

        .when('/sign_up', {
            templateUrl: '<%= asset_path("registrations/new.html.erb") %>',
            controller: 'RegistrationsCtrl'
        })

        .when('/sign_out', function () {

        })

        .otherwise({redirectTo: '/'});

//    $locationProvider.html5Mode(true);
})